        android:gravity="center_horizontal" />

    <Button
        android:id="@+id/plus4"
        android:layout_width="22dp"
        android:layout_height="37dp"
        android:background="@color/red"
        android:text="+"
        android:textColor="@color/yellow"
        android:textSize="20dp"
        app:layout_constraintBottom_toBottomOf="@+id/item4Image"
        app:layout_constraintEnd_toEndOf="@+id/item1Image"
        app:layout_constraintHorizontal_bias="0.938"
        app:layout_constraintStart_toStartOf="@+id/item2Image"
        app:layout_constraintTop_toTopOf="@+id/item4Image"
        app:layout_constraintVertical_bias="0.411" />

    <Button
        android:id="@+id/minus4"
        android:layout_width="22dp"
        android:layout_height="37dp"
        android:background="@color/red"
        android:text="-"
        android:textColor="@color/yellow"
        android:textSize="20dp"
        app:layout_constraintBottom_toBottomOf="@+id/item4Image"
        app:layout_constraintEnd_toEndOf="@+id/item4Image"
        app:layout_constraintHorizontal_bias="0.773"
        app:layout_constraintStart_toStartOf="@+id/item4Image"
        app:layout_constraintTop_toTopOf="@+id/item4Image"
        app:layout_constraintVertical_bias="0.411" />

    <Button
        android:id="@+id/minus1"
        android:layout_width="22dp"
        android:layout_height="37dp"
        android:background="@color/red"
        android:text="-"
        android:textColor="@color/yellow"
        android:textSize="20dp"
        app:layout_constraintBottom_toBottomOf="@+id/item1Image"
        app:layout_constraintEnd_toEndOf="@+id/item1Image"
        app:layout_constraintHorizontal_bias="0.773"
        app:layout_constraintStart_toStartOf="@+id/item1Image"
        app:layout_constraintTop_toTopOf="@+id/item1Image"
        app:layout_constraintVertical_bias="0.423" />

    <TextView
        android:id="@+id/item2Text"
        android:layout_width="36dp"
        android:layout_height="45dp"
        android:textSize="20dp"
        android:textColor="@color/yellow"
        android:textAlignment="center"
        app:layout_constraintBottom_toBottomOf="@+id/item2Image"
        app:layout_constraintEnd_toEndOf="@+id/plus2"
        app:layout_constraintHorizontal_bias="0.523"
        app:layout_constraintStart_toStartOf="@+id/minus2"
        app:layout_constraintTop_toTopOf="@+id/item2Image"
        app:layout_constraintVertical_bias="1.0"
        android:gravity="center_horizontal" />

    <ImageButton
        android:id="@+id/addToCart"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.493"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/item4Image"
        app:layout_constraintVertical_bias="0.086"
        app:srcCompat="@drawable/addtocart" />
</androidx.constraintlayout.widget.ConstraintLayout>