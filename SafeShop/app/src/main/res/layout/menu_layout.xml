        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/item3Image"
        app:layout_constraintVertical_bias="0.155"
        app:srcCompat="@drawable/menuitem4" />

    <TextView
        android:id="@+id/item4Text"
        android:layout_width="36dp"
        android:layout_height="45dp"
        android:textSize="20dp"
        android:textColor="@color/yellow"
        android:textAlignment="center"
        app:layout_constraintBottom_toBottomOf="@+id/item4Image"
        app:layout_constraintEnd_toEndOf="@+id/plus2"
        app:layout_constraintHorizontal_bias="0.523"
        app:layout_constraintStart_toStartOf="@+id/minus2"
        app:layout_constraintTop_toTopOf="@+id/item4Image"
