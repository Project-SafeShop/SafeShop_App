        app:layout_constraintBottom_toBottomOf="@+id/item3Image"
        app:layout_constraintEnd_toEndOf="@+id/plus2"
        app:layout_constraintHorizontal_bias="0.523"
        app:layout_constraintStart_toStartOf="@+id/minus2"
        app:layout_constraintTop_toTopOf="@+id/item3Image"
        app:layout_constraintVertical_bias="0.0"
        android:gravity="center_horizontal" />

    <Button
        android:id="@+id/plus3"
        android:layout_width="22dp"
        android:layout_height="37dp"
        android:background="@color/red"
        android:text="+"
        android:textColor="@color/yellow"
        android:textSize="20dp"
        app:layout_constraintBottom_toBottomOf="@+id/item3Image"
        app:layout_constraintEnd_toEndOf="@+id/item1Image"
        app:layout_constraintHorizontal_bias="0.938"
        app:layout_constraintStart_toStartOf="@+id/item2Image"
        app:layout_constraintTop_toTopOf="@+id/item3Image" />

    <Button
        android:id="@+id/minus3"
        android:layout_width="22dp"
        android:layout_height="37dp"
        android:background="@color/red"
        android:text="-"
        android:textColor="@color/yellow"
        android:textSize="20dp"
        app:layout_constraintBottom_toBottomOf="@+id/item3Image"
        app:layout_constraintEnd_toEndOf="@+id/item3Image"
        app:layout_constraintHorizontal_bias="0.775"
        app:layout_constraintStart_toStartOf="@+id/item3Image"
        app:layout_constraintTop_toTopOf="@+id/item3Image" />

